<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cgvak.dinesh" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <name>ToDoL</name>
    <description>
        A Simple ToDo application using WebSQL.
    </description>
    <author email="dineshrajaa.93@gmail.com" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
